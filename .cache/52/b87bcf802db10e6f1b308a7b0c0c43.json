red {\r\n  color: var(--accent-red)\n}\r\n\r\na:focus {\r\n  outline: none\n}\r\n\r\na.bouquets {\r\n  display: block;\r\n  max-width: 250px;\r\n  margin-top: 2rem;\r\n  padding: 0.75rem 1.5rem;\r\n\r\n  color: var(--main-light);\r\n  font-weight: 800;\r\n  line-height: 1;\r\n  text-align: center;\r\n  text-decoration: none;\r\n\r\n  background-color: var(--accent-red);\r\n\r\n  transition: background-color ease-in 0.1s, color ease-in 0.1s, box-shadow ease-in 0.1s\n}\r\n\r\na.bouquets:hover {\r\n  color: var(--accent-red);\r\n\r\n  background-color: var(--main-dark)\n}\r\n\r\na.bouquets:focus {\r\n  box-shadow: inset 0 0 0 0.25rem var(--main-dark)\n}\r\n\r\na:not(.bouquets):focus {\r\n  color: var(--accent-red)\n}\r\n\r\nsection.quotes {\r\n  margin-top: 3rem\n}\r\n\r\n@media screen and (min-width: 901px) {\r\n  section.quotes {\r\n    column-count: 3;\r\n    column-gap: 1rem\n  }\n}\r\n\r\nsection.quotes blockquote {\r\n  display: inline-block;\r\n  margin-top: 2rem;\r\n  margin-right: 0;\r\n  margin-bottom: 0;\r\n  margin-left: 0\n}\r\n\r\nsection.quotes p {\r\n  display: block;\r\n\r\n  font-size: 1.5rem;\r\n  line-height: 1.25\n}\r\n\r\nsection.quotes cite {\r\n  margin-top: 0.25rem\n}\r\n\r\nsection.who {\r\n  background-color: var(--accent-red)\n}\r\n\r\nsection:not(.review) {\r\n  color: var(--main-dark)\n}\r\n\r\nsection.review {\r\n  background-color: var(--main-dark)\n}\r\n\r\nsection.review h2, section.review p, section.review cite {\r\n  color: var(--main-light)\n}\r\n\r\nimg {\r\n  display: block;\r\n  width: 100%;\r\n  margin-top: 1rem;\r\n\r\n  border: 0.25rem solid var(--main-dark)\n}\r\n\r\ncite {\r\n  display: block\n}\r\n\r\nsection.bouquets {\r\n  background-color: var(--accent-pink)\n}\r\n\r\nsection.items {\r\n  display: grid;\r\n  grid-gap: 2rem 1rem\n}\r\n\r\nsection > span {\r\n  margin-top: 2rem;\r\n  padding: 1rem;\r\n\r\n  font-size: 1.5rem;\r\n\r\n  border: 0.25rem solid var(--accent-red)\n}\r\n\r\n@media screen and (min-width: 901px) {\